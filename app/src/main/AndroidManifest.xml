<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.syntheticknowledge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/mainStyle.noActionBarCustom"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".View.CreateAccount.SignUpActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".View.MainSystem.SearchActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".View.MainSystem.FragmentMain.NewFeedFragment.WriterStatusActivity"
            android:theme="@style/mainStyle.postActionBar"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".View.MainSystem.MainActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".View.LoginActivity"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>